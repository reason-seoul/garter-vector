{
  "name": "rescript-vector",
  "version": "0.2.0",
  "description": "Persistent Vector implemenetation in ReScript",
  "license": "MIT",
  "author": {
    "name": "Hyunwoo Nam"
  },
  "keywords": [
    "rescript",
    "data-structure",
    "immutable",
    "vector"
  ],
  "source": "./src/Vector.gen.ts",
  "types": "./dist/Vector.gen.d.ts",
  "main": "./dist/vector.js",
  "module": "./dist/vector.mjs",
  "exports": {
    ".": {
      "require": "./dist/vector.js",
      "import": "./dist/vector.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "postinstall": "rescript build -with-deps",
    "build": "rescript build",
    "bundle": "yarn build && concurrently -k \"yarn:bundle:*\"",
    "bundle:cjs": "esbuild src/Vector.gen.ts --bundle --sourcemap --minify --format=cjs --platform=node --target=node12 --outfile=dist/vector.js",
    "bundle:esm": "esbuild src/Vector.gen.ts --bundle --sourcemap --minify --format=esm --outfile=dist/vector.mjs",
    "bundle:dts": "tsc --emitDeclarationOnly",
    "prepack": "yarn build && yarn bundle",
    "start": "rescript build -w",
    "clean": "rescript clean",
    "pretest": "yarn build",
    "test": "node tests/vector_test.mjs | faucet",
    "test:watch": "yarn test || true && watchlist src tests -- yarn test",
    "test:coverage": "c8 yarn test"
  },
  "files": [
    "dist",
    "src/*.res",
    "src/*.resi",
    "bsconfig.json"
  ],
  "dependencies": {
    "@rescript/std": "^9.1.3"
  },
  "devDependencies": {
    "@dusty-phillips/rescript-zora": "^1.1.0",
    "c8": "^7.7.3",
    "concurrently": "^6.2.0",
    "esbuild": "^0.12.17",
    "faucet": "^0.0.1",
    "gentype": "^4.1.0",
    "rescript": "^9.1.4",
    "typescript": "^4.3.5",
    "watchlist": "^0.2.3"
  }
}
