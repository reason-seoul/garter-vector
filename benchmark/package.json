    "build": "yarn re:build && parcel build src/index.html --public-url ./",
    "deploy": "yarn build && gh-pages -d dist"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "bs-platform": "^9.0.0",
    "bsb-js": "^1.1.7",
    "gh-pages": "^3.1.0",
    "parcel": "^1.12.4",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^22.0.0"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "mori": "^0.3.2",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reason-react": "0.9.1",
    "rescript-vector": "^0.1.0"
  }
}
